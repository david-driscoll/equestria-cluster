{
  "app": {
    "adminEmailAddresses": ["david.driscoll@gmail.com"],
    "baseURL": "https://${APP}.${ROOT_DOMAIN}",
    "disableSignup": true,
    "keepCalendarEventDays": 0,
    "keepCookMealPlanDays": 0,
    "sessionExpirationDefault": "7d",
    "sessionExpirationRememberMe": "30d"
  },
  "cli": {
    "logLevel": "debug"
  },
  "cache": {
    "memoryMaxSizeMB": 512
  },
  "database": {
    "uri": "{{ .postgres_uriql }}"
  },
  "oidc": {
    "authentik": {
      "displayName": "${CLUSTER_TITLE}",
      "clientID": "{{ .oidc_client_id }}",
      "clientSecret": "{{ .oidc_client_secret }}",
      "oidcIssuerURL": "{{ .oidc_issuer }}",
      "scopes": ["openid", "profile", "email"]
    }
  }
}
