{
  "server": {
    "externalDomain": "https://${APP}.${ROOT_DOMAIN}",
    "tlsCertDir": null,
    "requestSizeMaxMb": 100
  },
  "slack": {
    "enabled": false
  },
  "tracing": {
    "enabled": false
  },
  "scim": {
    "enabled": false
  },
  "oidc": {
    "enabled": true,
    "clientId": "{{ .oidc_client_id }}",
    "clientSecret": "{{ .oidc_client_secret }}",
    "authUrl": "{{ .oidc_auth_url }}",
    "tokenUrl": "{{ .oidc_token_url }}",
    "userinfoUrl": "{{ .oidc_userinfo_url }}",
    "scope": "openid email profile",
    "loginButtonText": "Login with Authentik",
    "autoRegister": true
  },
  "saml": {
    "enabled": false
  },
  "lockfileDir": "/data/windmill_cache"
}
