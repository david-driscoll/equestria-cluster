{
  "editor.bracketPairColorization.enabled": true,
  "files.associations": {
    "**/*.json5": "json5"
  },
  "files.trimTrailingWhitespace": true,
  "sops.defaults.ageKeyFile": "age.key",
  "sops.binPath": "/opt/homebrew/bin/sops",
  "sops.beta": false,
  "sops.creationEnabled": true,
  "vs-kubernetes": {
    "vs-kubernetes.kubeconfig": "./kubeconfig",
    "vs-kubernetes.knownKubeconfigs": [
      "./kubeconfig"
    ]
  },
  "mcp": {
    "servers": {
      "kubernetes": {
        "command": "npx",
        "args": [
          "mcp-server-kubernetes"
        ]
      }
    }
  },
  "path-autocomplete.disableUpOneFolder": false,
  "path-autocomplete.triggerOutsideStrings": true,
  "yaml.schemaStore.enable": true,
  "yaml.schemas": {
    "kubernetes": "./kubernetes/**/*.yaml"
  },
  "cSpell.words": [
    "onepassword",
    "scrypted"
  ],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#663399",
    "activityBar.background": "#663399",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#a77038",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#663399",
    "statusBar.background": "#663399",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#7f40bf",
    "statusBarItem.remoteBackground": "#663399",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "tab.activeBorder": "#663399",
    "titleBar.activeBackground": "#7f40bf",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#7f40bf99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#639"
}
